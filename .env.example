# Crawlbase API token (get from https://crawlbase.com/)
# Use the JavaScript token for Facebook Marketplace
CRAWLBASE_TOKEN=your_javascript_token_here

# Vercel Postgres connection (auto-populated by Vercel)
POSTGRES_URL=

# Cron job secret for authentication
CRON_SECRET=your_random_secret_here

# Anthropic API key for LLM analysis
ANTHROPIC_API_KEY=your_anthropic_key_here

# Prisma Database URL (Prisma.io with Accelerate)
PRISMA_DATABASE_URL=

# Discord webhook URL (for notifications)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN
